{
    "name": "client",
    "version": "0.0.0",
    "main": "index.js",
    "scripts": {
        "start": "ng serve --open",
        "format": "prettier --config ../.prettierrc.yaml --write \"src/**/*\"",
        "build": "ng build",
        "lint": "ng lint --type-check --format stylish",
        "lint:fix": "npm run lint -- --fix",
        "start:electron": "ng build --base-href ./ && electron .",
        "build:electron": "ng build --prod --base-href ./  && electron-packager . --asar=true --ignore=/node_modules --ignore=/e2e --ignore=/src --overwrite --out=./build"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^10.0.14",
        "@angular/cdk": "~10.2.7",
        "@angular/common": "^10.0.14",
